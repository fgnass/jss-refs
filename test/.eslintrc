{
  "extends": "airbnb-base/legacy",
  "env": {
    "browser": true,
    "qunit": true
  },
  "globals": {
    "jss": false,
    "jssRefs": false
  },
  "rules": {
    "comma-dangle": 0,
    "eqeqeq": [2, "smart"],
    "func-names": 0,
    "max-len": 0,
    "semi": [2, "never"],
    "vars-on-top": 0
  }
}
