{
  "extends": "airbnb-base",
  "rules": {
    "semi": [2, "never"],
    "eqeqeq": [2, "smart"],
    "no-eq-null": 0,
    "brace-style": [2, "stroustrup"],
    "no-use-before-define": [2, "nofunc"],
    "block-scoped-var": 0,
    "no-param-reassign": 0,
    "guard-for-in": 0,
    "no-unexpected-multiline": 2,
    "comma-dangle": 0
  }
}
